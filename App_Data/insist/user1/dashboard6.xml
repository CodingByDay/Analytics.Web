        <CalculatedField Name="PlačeUreNeopredeljeno" Expression="Iif (PlačeVrstaDela='Neopredeljeno' or PlačeVrstaDela='DRUGO',PlačeUreFond,0)" DataType="Auto" DataMember="uTN_Analitika_Plače_Skupna" />
